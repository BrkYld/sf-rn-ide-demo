{
  "#pgHome-headerBar": {
    "backgroundColor": "#009142",
    "alignSelf": "stretch",
    "flex": 1,
    "flexShrink": 1,
    "visible": false
  },
  "#pgHome": {
    "&-headerBar": {
      "backgroundColor": "${mainColor}"
    },
    "&-customHeader": {
      "backgroundColor": "${mainColor}"
    },
    "&-iconContainer": {
      "backgroundColor": "${mainColor}"
    },
    "&-iconsContainer": {
      "backgroundColor": "${mainColor}"
    },
    "&-segmentContainer": {
      "backgroundColor": "${mainColor}"
    },
    "&-solidMainBackground": {
      "backgroundColor": "${mainColor}"
    },
    "&-txtGotoAccount": {
      "color": "${iconColor}"
    },
    "&-pgHome": {
      "backgroundColor": "${white}"
    },
    "&-accountCard": {
      "backgroundColor": "${white}",
      "borderColor": "${backgroundColor}"
    },
    "&-txtGoToAccountActivities": {
      "color": "${iconColor}"
    }
  },
  "#pgHome-headerView": {
    "height": 44,
    "width": null,
    "flexDirection": "row",
    "alignItems": "stretch"
  },
  "#pgHome-customHeader": {
    "width": null,
    "height": 150,
    "backgroundColor": "#009142",
    "flexDirection": "row",
    "alignItems": "flex-end",
    "justifyContent": "center",
    "paddingLeft": 10,
    "paddingRight": 10
  },
  "#pgHome-pgHome": {
    "paddingLeft": 0,
    "paddingRight": 0,
    "backgroundColor": "#ffffff"
  },
  "#pgHome-iconContainer": {
    "width": null,
    "height": 100,
    "flex": 1,
    "flexShrink": 1,
    "backgroundColor": "#009142"
  },
  "#pgHome-segmentContainer": {
    "width": null,
    "height": 50,
    "backgroundColor": "#009142",
    "flexDirection": "row",
    "alignItems": "center",
    "paddingLeft": 10
  },
  "#pgHome-iconsContainer": {
    "flex": 1,
    "flexShrink": 1,
    "backgroundColor": "#009142",
    "height": 75,
    "flexDirection": "row"
  },
  "#pgHome-leftSide": {
    "flexDirection": "row",
    "alignItems": "center",
    "justifyContent": "flex-start",
    "height": null,
    "width": null,
    "flex": 1,
    "flexShrink": 1
  },
  "#pgHome-rightSide": {
    "flexDirection": "row",
    "alignItems": "center",
    "justifyContent": "flex-end",
    "flex": 1,
    "flexShrink": 1,
    "height": null,
    "width": null
  },
  "#pgHome-segmentActive": {
    "color": "rgba( 255, 255, 255, 1 )",
    "fontSize": 20,
    "marginRight": 5
  },
  "#pgHome-segmentPassive": {
    "fontSize": 18,
    "color": "rgba( 255, 255, 255, 0.54 )"
  },
  "#pgHome-solidMainBackground": {
    "height": 150,
    "width": null,
    "backgroundColor": "#009142"
  },
  "#pgHome-accountCard": {
    "height": 300,
    "position": "absolute",
    "backgroundColor": "#ffffff",
    "top": 200,
    "width": 380,
    "left": 25,
    "borderRadius": 8,
    "borderWidth": 1,
    "borderColor": "rgba( 231, 231, 231, 1 )"
  },
  "#pgHome-titleContainer": {
    "width": null,
    "height": 50,
    "flexDirection": "row",
    "justifyContent": "space-between",
    "alignItems": "center",
    "padding": 10
  },
  "#pgHome-image2": {
    "width": 25,
    "height": 25,
    "tintColor": "rgba( 155, 155, 155, 0.69 )"
  },
  "#pgHome-accountDetailContainer": {
    "width": null,
    "height": 150,
    "padding": 10
  },
  "#pgHome-txtBalanceDesc": {
    "color": "rgba( 155, 155, 155, 1 )",
    "fontSize": 15,
    "marginBottom": 15
  },
  "#pgHome-txtBalance": {
    "fontSize": 25,
    "marginBottom": 15
  },
  "#pgHome-txtGotoAccount": {
    "fontSize": 17,
    "color": "rgba( 67, 179, 153, 1 )"
  },
  "#pgHome-lastActionContainer": {
    "width": null,
    "height": 95,
    "flexDirection": "row",
    "justifyContent": "space-around"
  },
  "#pgHome-bottomContainer": {
    "alignSelf": "stretch",
    "position": "relative",
    "width": null,
    "backgroundColor": "rgba( 255, 255, 255, 0 )",
    "marginTop": 150,
    "height": 400,
    "padding": 10
  },
  "#pgHome-lastActionCard": {
    "width": null,
    "height": 200,
    "padding": 10
  },
  "#pgHome-lastActionTitleContainer": {
    "height": 50,
    "width": null,
    "flexDirection": "row",
    "alignItems": "center"
  },
  "#pgHome-txtLastAction": {
    "fontSize": 20
  },
  "#pgHome-txtDate": {
    "fontSize": 15,
    "color": "rgba( 74, 74, 74, 0.68 )"
  },
  "#pgHome-lastActionDetailContainer": {
    "width": null,
    "height": 125
  },
  "#pgHome-lastActionDetailTypeContainer": {
    "width": null,
    "height": 35,
    "flexDirection": "row",
    "alignItems": "center",
    "justifyContent": "space-between"
  },
  "#pgHome-txtTypeDesc": {
    "fontSize": 18
  },
  "#pgHome-txtTypeDetail": {
    "fontSize": 18,
    "color": "rgba( 155, 155, 155, 0.66 )",
    "marginTop": 5
  },
  "#pgHome-txtGoToAccountActivities": {
    "fontSize": 15,
    "color": "rgba( 67, 179, 153, 1 )",
    "marginTop": 5
  }
}